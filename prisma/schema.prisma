datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider = "prisma-client-js"
}

model Feedback {
  id            Int      @id @default(autoincrement())
  name          String
  rating        Int
  comments      String
  published_date DateTime @default(now())
}