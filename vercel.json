{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "views/index.php"
        }
    ],
    "functions": {
        "api/**/*.php": {
            "runtime": "nodejs16.x"
        }
    }
}
