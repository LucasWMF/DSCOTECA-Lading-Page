{
    "functions": {
      "/viewsapi/index.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/views/api/index.php",
        "dest": "/views/api/index.php"
      },
      {
        "src": "/views/api/(.*)",
        "dest": "/views/api/index.php"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/image/(.*)",
        "dest": "/public/image/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/views/api/index.php"
      }
    ]
  }
  