{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/api/index.php"
        },
        {
            "src": "/root.css",
            "dest": "/public/css/root.css"
        },
        {
            "src": "/assents/img/(.*).png",
            "dest": "/public/image/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}